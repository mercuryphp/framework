<?xml version="1.0" encoding="UTF-8" ?>
<locale>
    <name>es-US</name>
    <displayName>Spanish (United States)</displayName>
    <datetime>
        <nativeCalendar>calendario gregoriano</nativeCalendar>
        <shortDatePattern>M/d/yyyy</shortDatePattern>
        <longDatePattern>dddd, MMMM dd, yyyy</longDatePattern>
        <dateSeparator>/</dateSeparator>
        <shortTimePattern>h:mm tt</shortTimePattern>
        <longTimePattern>h:mm:ss tt</longTimePattern>
        <timeSeparator>:</timeSeparator>
        <dayNames>
            <fullName>
                <name>domingo</name>
                <name>lunes</name>
                <name>martes</name>
                <name>miércoles</name>
                <name>jueves</name>
                <name>viernes</name>
                <name>sábado</name>
            </fullName>
            <shortName>
                <name>dom</name>
                <name>lun</name>
                <name>mar</name>
                <name>mié</name>
                <name>jue</name>
                <name>vie</name>
                <name>sáb</name>
            </shortName>
        </dayNames>
        <monthNames>
            <fullName>
                <name>enero</name>
                <name>febrero</name>
                <name>marzo</name>
                <name>abril</name>
                <name>mayo</name>
                <name>junio</name>
                <name>julio</name>
                <name>agosto</name>
                <name>septiembre</name>
                <name>octubre</name>
                <name>noviembre</name>
                <name>diciembre</name>
            </fullName>
            <shortName>
                <name>ene</name>
                <name>feb</name>
                <name>mar</name>
                <name>abr</name>
                <name>may</name>
                <name>jun</name>
                <name>jul</name>
                <name>ago</name>
                <name>sep</name>
                <name>oct</name>
                <name>nov</name>
                <name>dic</name>
            </shortName>
        </monthNames>
    </datetime>
    <numberFormat>
        <numberDecimalDigits>2</numberDecimalDigits>
        <numberDecimalSeparator>.</numberDecimalSeparator>
        <numberGroupSeparator>,</numberGroupSeparator>
        <currencySymbol>$</currencySymbol>
        <currencyDecimalDigits>2</currencyDecimalDigits>
        <currencyDecimalSeparator>.</currencyDecimalSeparator>
        <currencyGroupSeparator>,</currencyGroupSeparator>
        <currencyNegativePattern>0</currencyNegativePattern>
        <currencyPositivePattern>0</currencyPositivePattern>
    </numberFormat>
</locale>
