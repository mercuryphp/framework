<?xml version="1.0" encoding="UTF-8" ?>
<locale>
    <name>bo-CN</name>
    <displayName>Tibetan (PRC)</displayName>
    <datetime>
        <nativeCalendar>ལོ་ཐོ</nativeCalendar>
        <shortDatePattern>yyyy/M/d</shortDatePattern>
        <longDatePattern>yyyy'ལོའི་ཟླ' M'ཚེས' d</longDatePattern>
        <dateSeparator>/</dateSeparator>
        <shortTimePattern>HH:mm</shortTimePattern>
        <longTimePattern>HH:mm:ss</longTimePattern>
        <timeSeparator>:</timeSeparator>
        <dayNames>
            <fullName>
                <name>གཟའ་ཉི་མ།</name>
                <name>གཟའ་ཟླ་བ།</name>
                <name>གཟའ་མིག་དམར།</name>
                <name>གཟའ་ལྷག་པ།</name>
