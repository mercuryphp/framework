        <nativeCalendar>gregorijanski koledar</nativeCalendar>
        <shortDatePattern>d.M.yyyy</shortDatePattern>
        <longDatePattern>d. MMMM yyyy</longDatePattern>
        <dateSeparator>.</dateSeparator>
        <shortTimePattern>H:mm</shortTimePattern>
        <longTimePattern>H:mm:ss</longTimePattern>
        <timeSeparator>:</timeSeparator>
        <dayNames>
            <fullName>
                <name>nedelja</name>
                <name>ponedeljek</name>
                <name>torek</name>
                <name>sreda</name>
                <name>četrtek</name>
                <name>petek</name>
                <name>sobota</name>
            </fullName>
            <shortName>
                <name>ned</name>
                <name>pon</name>
                <name>tor</name>
                <name>sre</name>
                <name>čet</name>
                <name>pet</name>
                <name>sob</name>
            </shortName>
        </dayNames>
        <monthNames>
            <fullName>
                <name>januar</name>
                <name>februar</name>
                <name>marec</name>
                <name>april</name>
                <name>maj</name>
                <name>junij</name>
                <name>julij</name>
                <name>avgust</name>
                <name>september</name>
                <name>oktober</name>
                <name>november</name>
                <name>december</name>
            </fullName>
            <shortName>
                <name>jan</name>
                <name>feb</name>
                <name>mar</name>
                <name>apr</name>
                <name>maj</name>
                <name>jun</name>
                <name>jul</name>
                <name>avg</name>
                <name>sep</name>
                <name>okt</name>
                <name>nov</name>
                <name>dec</name>
            </shortName>
        </monthNames>
    </datetime>
    <numberFormat>
        <numberDecimalDigits>2</numberDecimalDigits>
        <numberDecimalSeparator>,</numberDecimalSeparator>
        <numberGroupSeparator>.</numberGroupSeparator>
        <currencySymbol>€</currencySymbol>
        <currencyDecimalDigits>2</currencyDecimalDigits>
        <currencyDecimalSeparator>,</currencyDecimalSeparator>
        <currencyGroupSeparator>.</currencyGroupSeparator>
        <currencyNegativePattern>8</currencyNegativePattern>
        <currencyPositivePattern>3</currencyPositivePattern>
    </numberFormat>
</locale>
