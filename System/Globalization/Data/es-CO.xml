<?xml version="1.0" encoding="UTF-8" ?>
<locale>
    <name>es-CO</name>
    <displayName>Spanish (Colombia)</displayName>
    <datetime>
        <nativeCalendar>calendario gregoriano</nativeCalendar>
        <shortDatePattern>dd/MM/yyyy</shortDatePattern>
        <longDatePattern>dddd, dd' de 'MMMM' de 'yyyy</longDatePattern>
        <dateSeparator>/</dateSeparator>
        <timeSeparator>:</timeSeparator>
        <dayNames>
            <fullName>
                <name>domingo</name>
                <name>lunes</name>
                <name>martes</name>
                <name>miércoles</name>
                <name>jueves</name>
                <name>viernes</name>
                <name>sábado</name>
            </fullName>
            <shortName>
                <name>dom</name>
                <name>lun</name>
                <name>mar</name>
                <name>mié</name>
                <name>jue</name>
                <name>vie</name>
                <name>sáb</name>
            </shortName>
        </dayNames>
        <monthNames>
            <fullName>
                <name>enero</name>
                <name>febrero</name>
                <name>marzo</name>
                <name>abril</name>
                <name>mayo</name>
                <name>junio</name>
                <name>julio</name>
                <name>agosto</name>
                <name>septiembre</name>
                <name>octubre</name>
                <name>noviembre</name>
                <name>diciembre</name>
            </fullName>
            <shortName>
                <name>ene</name>
                <name>feb</name>
                <name>mar</name>
                <name>abr</name>
                <name>may</name>
                <name>jun</name>
                <name>jul</name>
                <name>ago</name>
                <name>sep</name>
                <name>oct</name>
                <name>nov</name>
                <name>dic</name>
            </shortName>
        </monthNames>
    </datetime>
    <numberFormat>
        <numberDecimalDigits>2</numberDecimalDigits>
        <numberDecimalSeparator>,</numberDecimalSeparator>
        <numberGroupSeparator>.</numberGroupSeparator>
        <currencySymbol>$</currencySymbol>
        <currencyDecimalSeparator>,</currencyDecimalSeparator>
        <currencyGroupSeparator>.</currencyGroupSeparator>
        <currencyNegativePattern>14</currencyNegativePattern>
        <currencyPositivePattern>2</currencyPositivePattern>
    </numberFormat>
</locale>
