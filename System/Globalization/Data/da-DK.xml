<?xml version="1.0" encoding="UTF-8" ?>
<locale>
    <name>da-DK</name>
    <displayName>Danish (Denmark)</displayName>
    <datetime>
        <nativeCalendar>Den gregorianske kalender</nativeCalendar>
        <shortDatePattern>dd-MM-yyyy</shortDatePattern>
        <longDatePattern>d. MMMM yyyy</longDatePattern>
        <dateSeparator>-</dateSeparator>
        <shortTimePattern>HH:mm</shortTimePattern>
        <longTimePattern>HH:mm:ss</longTimePattern>
        <timeSeparator>:</timeSeparator>
        <dayNames>
            <fullName>
                <name>søndag</name>
                <name>mandag</name>
                <name>tirsdag</name>
                <name>onsdag</name>
                <name>torsdag</name>
                <name>fredag</name>
                <name>lørdag</name>
            </fullName>
            <shortName>
                <name>sø</name>
                <name>ma</name>
                <name>ti</name>
                <name>on</name>
                <name>to</name>
                <name>fr</name>
                <name>lø</name>
            </shortName>
        </dayNames>
        <monthNames>
            <fullName>
                <name>januar</name>
                <name>februar</name>
                <name>marts</name>
                <name>april</name>
                <name>maj</name>
                <name>juni</name>
                <name>juli</name>
                <name>august</name>
                <name>september</name>
                <name>oktober</name>
                <name>november</name>
                <name>december</name>
            </fullName>
            <shortName>
                <name>jan</name>
                <name>feb</name>
                <name>mar</name>
                <name>apr</name>
                <name>maj</name>
                <name>jun</name>
                <name>jul</name>
                <name>aug</name>
                <name>sep</name>
                <name>okt</name>
                <name>nov</name>
                <name>dec</name>
            </shortName>
        </monthNames>
    </datetime>
    <numberFormat>
        <numberDecimalDigits>2</numberDecimalDigits>
        <numberDecimalSeparator>,</numberDecimalSeparator>
        <numberGroupSeparator>.</numberGroupSeparator>
        <currencySymbol>kr.</currencySymbol>
        <currencyDecimalDigits>2</currencyDecimalDigits>
        <currencyDecimalSeparator>,</currencyDecimalSeparator>
        <currencyGroupSeparator>.</currencyGroupSeparator>
        <currencyNegativePattern>12</currencyNegativePattern>
        <currencyPositivePattern>2</currencyPositivePattern>
    </numberFormat>
</locale>
