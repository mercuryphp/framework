                <name>Selasa</name>
                <name>Rabu</name>
                <name>Kamis</name>
                <name>Jumat</name>
                <name>Sabtu</name>
            </fullName>
            <shortName>
                <name>Minggu</name>
                <name>Sen</name>
                <name>Sel</name>
                <name>Rabu</name>
                <name>Kamis</name>
                <name>Jumat</name>
